<Window x:Class="HDictInduction.Console.HumanEvalivationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Human Evaluation Helper"
        Width="718"
        Height="332"
        ResizeMode="NoResize">
    <Grid>
        <Button Name="button1"
                Width="88"
                Height="23"
                Margin="12,12,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="button1_Click"
                Content="Open File" />
        <Label Name="label_source"
               Height="28"
               Margin="106,11,181,0"
               VerticalAlignment="Top"
               Content="-" />
        <Rectangle Name="rectangle1"
                   Height="1"
                   Margin="12,44,6,0"
                   VerticalAlignment="Top"
                   Stroke="Black" />
        <Button Name="button_selectRandom"
                Width="106"
                Height="23"
                Margin="0,12,69,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Click="button_selectRandom_Click"
                Content="Select sample-&gt;" />
        <Label Name="label2"
               Height="28"
               Margin="0,107,12,0"
               HorizontalAlignment="Right"
               VerticalAlignment="Top"
               Content="U:"
               FlowDirection="RightToLeft" />
        <Label Name="label3"
               Height="28"
               Margin="0,153,12,0"
               HorizontalAlignment="Right"
               VerticalAlignment="Top"
               Content="K:"
               FlowDirection="RightToLeft" />
        <Label Name="label_u"
               Height="28"
               Margin="0,107,39,0"
               HorizontalAlignment="Right"
               VerticalAlignment="Top"
               Content="ئۇيغۇر"
               FlowDirection="RightToLeft"
               FontFamily="ALKATIP Tor Tom"
               FontSize="16" />
        <Label Name="label_k"
               Height="28"
               Margin="0,153,39,0"
               HorizontalAlignment="Right"
               VerticalAlignment="Top"
               Content="قازاق"
               FlowDirection="RightToLeft"
               FontFamily="ALKATIP Tor Tom"
               FontSize="16" />
        <Button Name="button_Yes"
                Width="75"
                Height="23"
                Margin="0,246,12,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Background="#FF9DE39D"
                Click="button_Yes_Click"
                Content="Yes"
                FontWeight="Bold" />
        <Button Name="button_no"
                Width="75"
                Height="23"
                Margin="0,246,110,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Background="#FFE8ABA7"
                Click="button_no_Click"
                Content="No"
                FontWeight="Bold" />
        <Label Name="label_index"
               Width="88"
               Margin="12,44,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Content="-"
               FontSize="16"
               FontWeight="Bold" />
        <ComboBox Name="comboBox1"
                  Width="57"
                  Height="23"
                  Margin="0,12,6,0"
                  HorizontalAlignment="Right"
                  VerticalAlignment="Top"
                  SelectedIndex="0">
            <ComboBoxItem Content="100" FontWeight="Bold" />
            <ComboBoxItem Content="50" />
            <ComboBoxItem Content="20" />
        </ComboBox>
    </Grid>
</Window>
